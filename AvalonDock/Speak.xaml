<Window x:Class="AvalonDock.Speak"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AvalonDock"
        mc:Ignorable="d"
        Title="Speak" Height="300" Width="300">
    <Grid>
        <Button x:Name="button" Content="Speak" HorizontalAlignment="Left" Margin="38,130,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="52" Margin="38,39,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="228"/>

    </Grid>
</Window>
